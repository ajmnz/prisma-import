{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "noUnusedLocals": true,
    "target": "es6",
    "outDir": "dist",
    "lib": [
      "es6"
    ],
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "composite": true,
    "declaration": true,
    "skipLibCheck": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}